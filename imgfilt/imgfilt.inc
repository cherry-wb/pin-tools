IMGFILT_DIR=$(TOP)/imgfilt
IMGFILT_LIB_A=libImgFilt.a
IMGFILT_LIB=$(IMGFILT_DIR)/$(IMGFILT_LIB_A)
IMGFILT_CC=$(IMGFILT_DIR)/ImgFilt.cc
IMGFILT_H=$(IMGFILT_DIR)/ImgFilt.h
IMGFILT_EXTRA=EXTRA_LPATHS=-L$(IMGFILT_DIR) EXTRA_LIBS=-lImgFilt EXTRA_LIB_DEPS=$(IMGFILT_LIB)

$(IMGFILT_LIB): $(IMGFILT_CC) $(IMGFILT_H)
	make -C $(IMGFILT_DIR) $(IMGFILT_LIB_A)